{
  "name": "module-two-grading-system",
  "version": "1.0.0",
  "description": "Automated grading scripts for React/Vite restaurant website projects (Module 2)",
  "main": "grading-scripts/generate-report.js",
  "scripts": {
    "test": "jest --verbose",
    "test:all": "jest --coverage",
    "grade": "node run-grading.js",
    "grade:unit": "node grading-scripts/run-unit-tests.js",
    "grade:gpt": "node grading-scripts/gpt-evaluator.js",
    "grade:report": "node grading-scripts/generate-report.js",
    "summarize": "node grading-scripts/code-summarizer.js",
    "analyze:git": "node grading-scripts/analyze-git-history.js",
    "install:grading": "npm install && cd grading-folder && npm install",
    "start": "node run-grading.js"
  },
  "dependencies": {
    "openai": "^4.20.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "jest-environment-node": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "grading",
    "automated-testing",
    "react",
    "vite",
    "education"
  ],
  "author": "Grading System",
  "license": "MIT"
}